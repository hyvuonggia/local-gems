# Local Gems Environment Variables
# Copy this file to .env and fill in your actual values

# Firebase Configuration
FIREBASE_API_KEY=your-firebase-api-key-here
FIREBASE_AUTH_DOMAIN=local-gems.firebaseapp.com
FIREBASE_PROJECT_ID=local-gems
FIREBASE_STORAGE_BUCKET=local-gems.appspot.com
FIREBASE_MESSAGING_SENDER_ID=123456789
FIREBASE_APP_ID=1:123456789:android:abcdef123456

# Google Maps API Key
GOOGLE_MAPS_API_KEY=your-google-maps-api-key-here

# App Configuration
APP_ENV=development
API_BASE_URL=https://api.localgems.com
DEBUG_MODE=true
